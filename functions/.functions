sudorecon() {
  sudo env "PATH=$PATH" autorecon $1
}

write-ftp() { 
  python3 -m pyftpdlib -p 21 -w 
}

get-ftp() { 
  python3 -m pyftpdlib -p 21 
  }

web-serve() {
  python3 -m http.server 80 
}

quicksmb() {
  echo -e "Quicksmb-share setup!"
  echo -e "\n Username : quick    Password: smb"
  smbserver.py -smb2support share .
  #-smb2support -username quick -password smb
}

smbscan() {
  echo -e "\n [++] Scanning SMB with nmap!: $1 \n"
  sudo nmap -v -p 139,445 -sSV --script smb-os-discovery,smb-enum-domains,smb-enum-groups,smb-enum-users,smb-enum-shares "$1"
}

ftpscan() {
  echo -e "\n [++] Scanning FTP with nmap!: $1 \n"
  sudo nmap -v -p 21 -sSVC --script ftp-anon "$1"
}

